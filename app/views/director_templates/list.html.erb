<h1>List of directors</h1>





<table border="1"> 
  <tr>
    <th>ID</th>
    <th>Image</th>
    <th>Name</th>
    <th>Dob</th>
    <th></th>
  </tr>

  <% Director.all.each do |director| %>
  <tr>
    <td><%= director.id %></td>
    <td>
      <img src="<%= director.image %>" width="100"%>
    </td>
    <td><%= director.name %></td>
    <td><%= director.dob %></td>
    <td>
      <%= link_to "Show Details", director_path(director) %>
      </td>
<% end %>



</table>
